 $contrl scftyp=rhf runtyp=gradient coord=zmt $end
 $system timlim=1 $end
 $basis  gbasis=sto ngauss=3 $end
 $guess  guess=huckel $end
 $scrf   radius=1.93 dielec=80.0 $end
